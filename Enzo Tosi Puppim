        "#Calculadora de IMC\n",
        "peso = float(input(\"Digite seu peso: \"))\n",
        "altura = float(input(\"Digite sua altura: \"))\n",
        "\n",
        "imc = peso / (altura * altura)\n",
        "\n",
        "print(\"seu imc é: \", imc)"
